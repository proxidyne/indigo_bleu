<?xml version="1.0"?>
<!-- If your plugin wants to add menu items to it's submenu off the new Extensions menu,
	 define them here. Each should have a unique menu id, a Name, and an Action. The last
	 is a method name in your python file that will be called when the user selects that
	 menu item. Note - nothing will be returned to the client, so if you need to communicate
	 back to the user you can post information into the Event Log.
-->
<MenuItems>
	<MenuItem id="turnOnSingleOutputMenu">
		<Name>Turn Single Relay On...</Name>
		<ConfigUI actionId="turnOnOutput"/>
	</MenuItem>
	<MenuItem id="turnOffSingleOutputMenu">
		<Name>Turn Single Relay Off...</Name>
		<ConfigUI actionId="turnOffOutput"/>
	</MenuItem>
	<MenuItem id="allOffMenu">
		<Name>Turn Off All Relays...</Name>
		<ConfigUI actionId="allOutputsOff"/>
	</MenuItem>
	<MenuItem id="allOnMenu">
		<Name>Turn On All Relays...</Name>
		<ConfigUI actionId="allOutputsOn"/>
	</MenuItem>
	<MenuItem id="changeMultiOutputMenu">
		<Name>Change Multiple Relays...</Name>
		<ConfigUI actionId="changeMultiple"/>
	</MenuItem>
</MenuItems>
